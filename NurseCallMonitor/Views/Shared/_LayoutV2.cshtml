<!--
  HOW TO USE:
  data-theme: default (default), dark, light, colored
  data-layout: fluid (default), boxed
  data-sidebar-position: left (default), right
  data-sidebar-layout: default (default), compact
-->
@{
    string fileName = DateTime.Now.ToShortDateString();
}

<body data-theme="colored" data-layout="fluid" data-sidebar-position="left" data-sidebar-layout="default">
    <!-- Modal -->
    <div class="wrapper">
        <nav id="sidebar" class="sidebar js-sidebar collapsed">
            <div class="sidebar-content js-simplebar">
                <ul class="sidebar-nav">
                    <a class="sidebar-brand" href="#">
                        <span class="sidebar-brand-text align-middle">
                            Nurse Call Monitor
                        </span>
                    </a>
                    @if (ViewData["Floors"] != null)
                    {
                        @foreach (var floorName in ViewData["Floors"] as IList<string>)
                        {
                            <li class="sidebar-item">
                                <a class="sidebar-link" href="@Url.ActionLink($"Index", "Home",new  { floorName= floorName})">
                                    <i class="align-middle" data-feather="user"></i> <span class="align-middle">@floorName</span>
                                </a>
                            </li>
                        }
                    }

                </ul>
                <div class="sidebar-cta">
                    <div class="sidebar-cta-content">
                        <div class="d-grid">
                            <a href="@Url.ActionLink($"AddFloor", "Home")" class="btn btn-outline-primary">Add Floor</a>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <div class="main">
            <nav class="navbar navbar-expand navbar-light navbar-bg">
                <a class="sidebar-toggle js-sidebar-toggle">
                    <i class="hamburger align-self-center"></i>
                </a>

                <div class="navbar-collapse collapse">
                    <ul class="navbar-nav navbar-align">
                        <li class="nav-item">
                            <a class="nav-icon js-fullscreen d-none d-lg-block" href="#">
                                <div class="position-relative">
                                    <i class="align-middle" data-feather="maximize"></i>
                                </div>
                            </a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-icon pe-md-0 dropdown-toggle" href="#" data-bs-toggle="dropdown">
                                <i class="fa fa-user fa-1x bg-info" aria-hidden="true"></i>
                            </a>
                            <div class="dropdown-menu dropdown-menu-end">
                                <a class="dropdown-item" href="@Url.ActionLink($"AddFloor", "Home",new { floorName= ViewData["FloorName"]}, protocol: "https")">
                                    <i class="align-middle me-1" data-feather=" "></i> Edit Floor
                                </a>
                                <div class="dropdown-divider"></div>
                                @*<span class="dropdown-item" href="@Url.ActionLink($"DeleteFloor", "Home",new { floorName= ViewData["FloorName"]}, protocol: "https")">
                                    <i class="align-middle me-1" data-feather=" minus-circle"></i> Delete Floor
                                </span>*@
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>

