    <style>
        .RoomCls {
            min-width: 170px !important;
            min-height: 110px !important;
            padding: 0px !important;
            display: inline-block;
            z-index: 1000;
        }

        .bed h2, h3, i {
            font-weight: bolder;
            color: white;
            z-index: 1000;
        }

        .draggable-heading {
            display: inline-block;
            font-weight: bolder;
            text-align: center;
            color: white !important
        }

            .draggable-heading > h2 {
                color: white !important
            }

        .room-label {
            font-weight: bolder;
            text-align: center;
            color: white !important;
            z-index: 1000; 

        }

        .floor {
            padding: 10px;
        }

        body {
            text-align: center;
        }

        .bed {
            box-shadow: rgba(17, 17, 26, 0.1) 0px 4px 16px, rgba(17, 17, 26, 0.1) 0px 8px 24px, rgba(17, 17, 26, 0.1) 0px 16px 56px;
            margin: 10px;
            text-align: center;
            min-width: 150px;
            float: left;
            z-index: 1000;
        }

        #draggable {
            cursor: n-resize;
        }

        h3 {
            clear: left;
        }

        .square-grid {
            border: 20px ridge #4b7aad;
            background: #121d2a;
            background-image: linear-gradient(#162332 1px, transparent 2px), linear-gradient(90deg, #162332 1px, transparent 1px);
            background-size: 20px 20px;
            min-height: 557px;
            height: 100%;
        }

        .full-size {
        }

        .close {
            top: -5px;
            right: -10px;
            position: absolute;
        }

        .edit {
            top: -5px;
            right: 5px;
            position: absolute;
        }

        .floor-box {
            min-width: 100px;
            border: 2px ridge #4b7aad;
            min-height: 100px;
            background: white;
            position: relative;
        }

        div.full-screen {
            z-index: 9999;
            width: 100%;
            height: 100%;
            position: fixed;
            top: 0;
            left: 0;
        }

        .text_container {
            z-index: 10;
        }

        .box_container, .text.container {
            display: inline-block;
        }
        .box-container {
            z-index: 10;
        }
        .full-icon {
            position: relative;
            top: 0px;
            color: white;
            float: right;
        }


        #containment-wrapper {
            width: 100%;
            position: relative;
        }

        .floor-object {
            position: absolute;
        }


    </style>
    <!-- Choose your prefered color scheme -->
